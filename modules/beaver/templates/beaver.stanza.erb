[<%= @source_real %>]
<% if @redis_url -%>
redis_url: <%= @redis_url %>
<% end -%>
<% if @redis_namespace -%>
redis_namespace: <%= @redis_namespace %>
<% end -%>
type: <%= @type %>
<% if @tags.is_a?(Array) -%>
tags: <%= @tags.join(',') %>
<% elsif @tags.is_a?(String) -%>
tags: <%= @tags %>
<% end -%>
<% if @format -%>
format: <%= @format %>
<% end -%>
<% if scope.lookupvar('beaver::enable_sincedb') -%>
sincedb_write_interval: <%= scope.lookupvar('sincedb_write_interval') %>
<% end -%>
<% unless @exclude.empty? -%>
<% if @exclude.is_a?(Array) -%>
exclude: (<%= @exclude.join('|') %>)
<% elsif @exclude.is_a?(String) -%>
exclude: (<%= @exclude %>)
<% end -%>
<% end -%>

